<Window x:Class="QactorSandBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QactorSandBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl >
            <TabItem Header="Test">
                <StackPanel>
                    <Button Name="btnVisitQactorSystemSpec" Content="VisitQactorSystemSpec" Click="btnVisitQactorSystemSpec_Click" />
                    <Button Name="btnVisitQactorProgSystemSpec" Content="VisitQactorProgSystemSpec" Click="btnVisitQactorProgSystemSpec_Click" />
                    <Button Name="btnCreateVisitor" Content="CreateVisitor" Click="btnCreateVisitor_Click" />
                </StackPanel>
                
            </TabItem>
            <TabItem Header="Toy">
                <StackPanel>
                    <Button Name="btnToy" Content="Toy" Click="btnToy_Click" />
                </StackPanel>

            </TabItem>
        </TabControl>
    </Grid>
</Window>
